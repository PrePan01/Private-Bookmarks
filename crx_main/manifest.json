{
    "manifest_version": 2,
    "name": "隐私书签",
    "version": "0.0.1",
    "description": "隐私书签",
    "icons": {
        "128": "img/星星pixel.png",
        "48": "img/星星pixel.png",
        "16": "img/星星pixel.png"
    },
    "browser_action": {
        "default_icon": "img/星星pixel.png",
        "default_popup": "popup/html/popup_index.html"
    },
    "background": {
        "scripts": [
            "popup/js/bg/bgjs.js"
        ]
    },
    "permissions": [
        "storage",
        "notifications",
        "tabs",
        "proxy",
        "activeTab",
        "webRequest",
        "webNavigation",
        "http://*/*"
    ]
}
